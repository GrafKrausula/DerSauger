  "name": "Der Sauger",
  "description": "Er saugt dir mit STRG+LEERTASTE alles aus dem Netz",
  "version": "1.0",
  "manifest_version": 3,
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },

  "action": {
    "default_title": "Der Sauger"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "toggle": {
      "description": "Direktes Sauging",
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "MacCtrl+Space"
      }
    },
    "wav": {
      "description": "Direktes Sauging und WAV-konvertiering",
      "suggested_key": {
        "default": "Shift+Alt+W",
        "mac": "Shift+Alt+W"
      }
    },
    "mp3": {
      "description": "Direktes Sauging und mp3-konvertiering",
      "suggested_key": {
        "default": "Shift+Alt+3",
        "mac": "Shift+Alt+3"
      }
    },
    "mp4": {
      "description": "Direktes Sauging und mp4-konvertiering",
      "suggested_key": {
        "default": "Shift+Alt+4",
        "mac": "Shift+Alt+4"
      }
    }
  },
  "icons": {
    "16": "images/Sauger_16.png",
    "32": "images/Sauger_32.png",
    "48": "images/Sauger_48.png",
    "128": "images/Sauger_128.png"
  },

  "permissions": ["tabs", "nativeMessaging", "activeTab", "storage"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
